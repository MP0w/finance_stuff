{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production tsc && npm run sentry:sourcemaps",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "tsc && npx nodemon -x ts-node -e ts .",
    "knex": "NODE_ENV=development NODE_OPTIONS='--import tsx/esm' knex",
    "migrate": "npm run knex migrate:latest",
    "codegen": "ts-node generateTypes.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org mpow --project node ./dist && sentry-cli sourcemaps upload --org mpow --project node ./dist",
    "update:connectors": "yarn remove finance_stuff_connectors && yarn add finance_stuff_connectors@https://github.com/MP0w/finance_stuff_connectors.git"
  },
  "dependencies": {
    "@sentry/cli": "^2.36.1",
    "@sentry/node": "^8.30.0",
    "@sentry/profiling-node": "^8.30.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "finance_stuff_connectors": "https://github.com/MP0w/finance_stuff_connectors.git",
    "firebase-admin": "^12.4.0",
    "knex": "^3.1.0",
    "node": "^22.8.0",
    "pg": "^8.12.0",
    "uuid": "^10.0.0",
    "uuid-by-string": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "@types/uuid": "^10.0.0",
    "knex-types": "^0.5.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.5.0"
}
